//https://codeforces.com/contest/1749/problem/A

#include <bits/stdc++.h>

using namespace std;
typedef long long ll;

void solve(){
	int n, m;
	cin >> n >> m;
	
	for(int i = 0; i < m; ++i) {
		int x, y;
		cin >> x >> y;
	}
	
	if(n > m) cout << "YES" <<endl;
	else cout << "NO" << endl;

}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    int tt;    
    cin >> tt;
    while(tt--){
        solve();
    }

    return 0;
}
