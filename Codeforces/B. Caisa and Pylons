//https://codeforces.com/contest/463/problem/B
#include <bits/stdc++.h>

using ll = long long;
using namespace std;

void solve() {
    int n; 
    cin >> n;

    int m = -1;
    for(int i = 0; i < n; ++i) {
        int x;
        cin >> x;
        m = max(x, m);
    }
   
    cout << m << endl;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();
    
    return 0;
}
