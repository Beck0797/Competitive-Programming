//http://codeforces.com/contest/237/problem/A
#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    cin >> n;
    map<int, int> mp;
    for(int i = 0; i < n; ++i){
        int h, m;
        cin >> h >> m;

        int time = h * 100 + m;
        mp[time]++;    
    }
    int max = -1;
    for(auto it = mp.begin(); it != mp.end(); ++it){
        if(it -> second > max){
            max = it -> second;
        }
    }
    
    cout << max << endl;
    return 0;
}
